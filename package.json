{
  "name": "homemadearcade-engine",
  "version": "0.1.0",
  "description": "HA engine with client-side prediction, lag compensation, pixi rendering",
  "scripts": {
    "start": "npm-run-all --parallel server client",
    "server": "nodemon ./server/index.js --ignore public",
    "client": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "author": "Jonathan Pedigo",
  "license": "MIT",
  "dependencies": {
    "esm": "3.2.25",
    "nengi": "1.13.1",
    "pixi.js": "5.1.5",
    "sat": "0.8.0"
  },
  "devDependencies": {
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "3.8.2",
    "webpack-merge": "4.2.2"
  }
}
